<div class="row">
	<div class="col-md-4">
		<span class="number_records white">{{Data.pagination.label_details}}</span>
	</div>
	<div class="col-md-8 text-right">
		<ul class="pagination">
			{% if Data.pagination.first != '' and Data.pagination.first != Data.pagination.current_page %}
			<li><a href="{{Data.pagination.first}}">{{Data.pagination.label_first}}</a></li>
			{% else %}
			<li class="disabled"><span>{{Data.pagination.label_first}}</span></li>
			{% endif %}
			{% if Data.pagination.previous != '' %}
			<li><a href="{{Data.pagination.previous}}">{{Data.pagination.label_previous}}</a></li>
			{% else %}
			<li class="disabled"><span>{{Data.pagination.label_previous}}</span></li>
			{% endif %}
			{% for item in Data.pagination.pages %}
			<li><a class="{% if item.current == 1 %}active{% endif %}" href="{{item.url}}">{{item.page}}</a></li>
			{% endfor %}
			{% if Data.pagination.next != '' %}	
			<li><a href="{{Data.pagination.next}}">{{Data.pagination.label_next}}</a></li>
			{% else %}
			<li class="disabled"><span>{{Data.pagination.label_next}}</span></li>
			{% endif %}
			{% if Data.pagination.last != '' and Data.pagination.last != Data.pagination.current_page %}
			<li><a href="{{Data.pagination.last}}">{{Data.pagination.label_last}}</a></li>
			{% else %}
			<li class="disabled"><span>{{Data.pagination.label_last}}</span></li>
			{% endif %}
		</ul>
	</div>	
</div>