
<div id="allofuser">
   
    {% for comp in Data.infocompany %}
        <div id="infouser">
            ID: {{ comp.id }} <br/>
            Nombre: {{ comp.nombre }} <br/>
            RFC: {{ comp.rfc }} <br/>
            Direccion: {{ comp.direccion }} <br/>
            Email:{{ comp.email }} <br/>
            Descripcion: {{ comp.descripcion }}<br/>
            Telefono: {{ comp.telefono }}<br/>
            Ubicacion: {{ comp.ubicacion }}<br/>
            Latitud: {{ comp.latitud }}<br/>
            Longitud: {{ comp.longitud }}<br/>
            Fecha de Creacion: {{ comp.fechacreacion }}<br/>
            {% if Data.photo is not empty %}
                {% for thephoto in Data.photo %}
                    Foto:<img width="30%" height="30%" src="http://static.publicameenvideo.local/data/tmp/{{ thephoto.nombre }}"> <br/>

                {% endfor %}

            {%else%} NO HAY IMAGEN DISPONI[BLE
            {% endif %}

        </div>
    {% endfor %}

</div>
