<h1>{{Data.inbox}}</h1>



<hr/>

<div class="fullw">    
    <span class="pull-left a80">
        <input type="text" id="buscador" onchange="Comm.search(this)" class="form-control" placeholder="Buscar">
    </span>

    <span class="pull-right a20">

        {% include('opcionesuser.twig') %}
    </span>
</div >






<div class="padding-top">
    <table class="table table-hover table-bordered">
        <thead>
            <tr>
                <th>ID</th>  
                <th>Nombre</th>
                <th>Descripcion</th>
                <th>Telefono</th>
                <th>E-mail</th>
                <th>Eliminar/Bloquear</th>
            </tr>
        </thead>
        <tbody>
            {% for empresa in Data.empresas %}
                <tr>
                    <td>{{ empresa.id }}</td> 
                    <td>{{ empresa.name }}</td> 
                    <td>{{ empresa.description }}</td>
                    <td>{{ empresa.phone }}</td>
                    <td>{{ empresa.email }}</td>
                    
                    <td><input class="checkbox" mainid="{{empresa.id}}" value="{{ empresa.name }}" type="checkbox"></input></td>

                </tr>
            {% endfor %}
        </tbody>
    </table>
</div>
        
        
        
        
        
        

<div class="center">       

    {% include('pager.twig') %}
</div>
<button onclick="drawRegionsMap();">BOTON</button>

<div id="regions_div" style="width:400; height:300"></div>

<div class="a100 ">

    {{Data.path}}
    <button type="button" onclick="GetChecked();" class="btn btn-primary pull-right" data-toggle="modal" data-target="#myModal">
        Agregar Usuario
    </button>


    <button type="button"  onclick="GetChecked();" class="btn btn-primary pull-right"  data-toggle="modal" data-target="#myModaldelete">
        Eliminar Usuarios
    </button>

    <button type="button" onclick="GetChecked();" class="btn btn-primary pull-right"  data-toggle="modal" data-target="#myModalBlock">
        Bloquear Usuarios
    </button>


</div>
