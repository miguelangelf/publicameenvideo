{% extends 'defaultAdmin.twig' %}



{% block content %}




    <div class="container padding-top">
        <div class="jumbotron">






            <h1>Hola de nuevo. <br/> <small> Por favor inicia sesión. </small></h1>

            <hr/>
            <div id="errorlogin" class="alert alert-danger" role="alert">Usuario o Contraseña Incorrecta</div>

            <form class="form-horizontal">




                <div class="form-group">
                    <label for="inputEmail3" class="col-sm-2 control-label">Email</label>
                    <div class="col-sm-10">
                        <input type="text" id="emaillogin" class="form-control" id="inputEmail3" placeholder="Email">
                    </div>
                </div>
                <div class="form-group">
                    <label for="inputPassword3" class="col-sm-2 control-label">Password</label>
                    <div class="col-sm-10">
                        <input type="password" id="passwordlogin" class="form-control" id="inputPassword3" placeholder="Password">
                    </div>
                </div>
                
                
                <div class="form-group">
                    <div class="col-sm-offset-2 col-sm-10">
                        <input type="button"  value="Ingresar" onclick="checklogin();" class="btn btn-default">
                    </div>
                </div>
            </form>



        </div>
    </div>

{% endblock %}